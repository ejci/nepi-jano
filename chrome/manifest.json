{
	"name": "Nepi Jano!",
	"description": "This extension allows to read paid content on www.sme.sk and www.hnonline.sk",
	"version": "0.9.3",
	"manifest_version": 2,
	"permissions": ["webRequest", "webRequestBlocking", "*://*.sme.sk/*"],
	"background": {
		"scripts": ["background.js"]
	},
	"content_scripts": [
		{
			"js": ["lib/jquery-2.0.0.min.js", "nepijano.js"],
			"matches": ["*://*.sme.sk/*", "*://*.hnonline.sk/*"]
		}
	],
	"app": {
		"launch": {
			"local_path": "app.html"
		}
	},
	"homepage_url": "https://bitbucket.org/miroslavmagda/nepi-jano",
	"icons": {
		"16": "icon_16.png",
		"48": "icon_48.png",
		"128": "icon_128.png"
	}
}
